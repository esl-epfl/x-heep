{
  target: {
    name: "pynq-z2"
    type: "fpga"
    usbPort: 2 
    baudrate: 9600
    iterations: 1
    outputFile: "sim.log"
  }

  report: {
    dir: "report"
  }
  
  tests: [
    {
      appName: "example_asm"
      dir: "../sw/applications/example_asm"
      outputFormat: "(\\d+):(\\d+)" 
      outputTags: [
        "ID",
        "Outcome"
      ]
    },
